<!--pages/question/index.wxml-->
<view class='body' style='background: url(https://7770-wp-test-32ff30-1259082207.tcb.qcloud.la/image/questionandshare/question_background.jpg?sign=00925f460e7e23b1602a7d863e45d8e2&t=1556415673)'>
  <view class='list-item' wx:for='{{question_naire}}' bindtap='choose_question'>
    <image class='left'   
    src='https://7770-wp-test-32ff30-1259082207.tcb.qcloud.la/image/soulMatch-user/wp.jpeg? sign=399f0b5c1b5b0c9117a993f67f772dc9&t=1556466101'></image>
    <view class='right'>
      <text id='name'>{{item._openid}}</text>
    </view>
  </view>
  <view class='question-answer'>
    <button id='submit_question' bindtap='submit_question'>我的灵魂</button>
    <button id='answer_question' bindtap='answer_question'>开始匹配</button>
  </view>
</view>